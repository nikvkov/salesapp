<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ExsalesMobileApp.pages.functions.components.AddBonusPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lb_title"/>
            <Entry x:Name="en_title" IsEnabled="False"/>
            <Label x:Name="lb_ean"/>
            <Entry x:Name="en_ean" IsEnabled="False"/>
            <Label x:Name="lb_bonus"/>
            <Entry x:Name="en_bonus" Keyboard="Numeric"/>
            <Button x:Name="bt_save"/>
            <Button x:Name="bt_back"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>